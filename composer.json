{
    "name": "mjmarianetti/avalara-php",
    "description": "Wrapper for Avalara Tax API",
    "license": "MIT",
    "homepage": "http://github.com/mjmarianetti/avalara-php",
    "keywords": [
        "PHP",
        "package",
        "Avalara",
        "API"
    ],
    "authors": [{
        "name": "Martin Marianetti",
        "email": "martin.marianetti@gmail.com"
    }],
    "require": {
        "php": "^5.3 | ^7.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Mjmarianetti\\Avalara\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Mjmarianetti\\Avalara\\Tests\\": "tests/"
        }
    }
}
